<?xml version="1.0" ?>


<hibernate-mapping package="zxc.tour.bean">
    <class name="zxc.tour.bean.ArticleBean" table="article">
        <id name="id" type="java.lang.String" column="id"/>
        <property name="articleContent" type="java.lang.String" column="content"/>
        <property name="articleTitle" type="java.lang.String" column="title"/>
    </class>
</hibernate-mapping>